{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "typeRoots": [
      "./types"
    ],
    "paths": {
      "@app/screens/*": [
        "./screens/*"
      ],
      "@app/components/*": [
        "./components/*"
      ],
      "@app/theme/*": [
        "./theme/*"
      ],
      "@app/types/*": [
        "./types/*"
      ],
    },
  }
}
